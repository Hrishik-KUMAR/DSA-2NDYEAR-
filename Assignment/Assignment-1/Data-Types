#include <iostream>
#include <cstdio>
using namespace std;


 int main() 
{ 
    int  a;
    long  b;
    char ch;
    float d;
    double e;
    
    scanf("%d %ld %c %f %lf",&a, &b, &ch, &d, &e);
    printf("%d\n%ld\n%c\n%f\n%lf", a, b, ch, d, e);
     
    
    return 0;
}
